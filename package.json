{
  "name": "project-atelier",
  "version": "1.0.0",
  "description": "HackReactor FEC Project",
  "main": "src/index.js",
  "scripts": {
    "react-dev": "webpack --mode development --watch",
    "react-dev:hot": "npx webpack serve --hot -open",
    "test": "npx jest --setupFiles dotenv/config",
    "test:coverage": "npx jest --coverage",
    "create-badges": "npm run test:coverage && npx istanbul-badges-readme",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Team-Daffodil/project-atelier.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Team-Daffodil/project-atelier/issues"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "homepage": "https://github.com/Team-Daffodil/project-atelier#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack4": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack4": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "babel-loader": "^8.2.1",
    "husky": "^8.0.2",
    "istanbul-badges-readme": "^1.8.4",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.4",
    "msw": "^0.49.1",
    "prettier-eslint": "^15.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  }
}
